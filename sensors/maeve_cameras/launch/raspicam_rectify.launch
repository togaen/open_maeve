<launch>
	<node
		pkg="image_transport"
		type="republish"
		name="republish"
    args="compressed in:=/raspicam_node/image out:=/raspicam_node/image_raw"/>
  <node pkg="image_proc" type="image_proc" name="image_proc" ns="raspicam_node">
    <remap from="/raspicam_node/image" to="/raspicam_node/image_raw"/>
  </node>
</launch>
